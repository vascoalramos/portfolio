{
  "name": "portfolio",
  "private": true,
  "description": "Personal Website",
  "version": "3.2.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vascoalramos/portfolio"
  },
  "bugs": {
    "url": "https://github.com/vascoalramos/portfolio/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build && gh-pages -d public -m 'Deploy new portfolio version'",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,css,md}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "bootstrap": "^5.2.3",
    "gatsby": "^5.9.1",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-image": "^3.9.0",
    "gatsby-plugin-manifest": "^5.9.0",
    "gatsby-plugin-offline": "^6.9.0",
    "gatsby-plugin-robots-txt": "1.8.0",
    "gatsby-plugin-sass": "^6.9.0",
    "gatsby-plugin-sharp": "^5.9.0",
    "gatsby-plugin-sitemap": "^6.9.0",
    "gatsby-remark-code-titles": "^1.1.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^7.9.0",
    "gatsby-source-filesystem": "^5.9.0",
    "gatsby-transformer-remark": "^6.9.0",
    "gatsby-transformer-sharp": "^5.9.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-reveal": "^1.2.2",
    "react-spring": "^9.7.1",
    "react-transition-group": "^4.4.5",
    "scrollreveal": "^4.0.9",
    "styled-components": "^5.3.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.8",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "react-is": "^18.2.0",
    "sass": "^1.62.1"
  }
}
