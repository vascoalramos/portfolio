{
  "name": "portfolio",
  "private": true,
  "description": "Personal Website",
  "version": "3.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vascoalramos/portfolio"
  },
  "bugs": {
    "url": "https://github.com/vascoalramos/portfolio/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build && gh-pages -d public -m 'Deploy new portfolio version'",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,css,md}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "bootstrap": "^5.2.3",
    "gatsby": "^4.24.8",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-image": "^2.18.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-offline": "^5.24.0",
    "gatsby-plugin-react-helmet": "^5.18.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sass": "^5.18.0",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-plugin-sitemap": "^5.24.0",
    "gatsby-remark-code-titles": "^1.1.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^6.18.0",
    "gatsby-source-filesystem": "^4.18.0",
    "gatsby-transformer-remark": "^5.18.0",
    "gatsby-transformer-sharp": "^4.24.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.6.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-reveal": "^1.2.2",
    "react-spring": "^9.4.5",
    "react-transition-group": "^4.4.5",
    "scrollreveal": "^4.0.9",
    "styled-components": "^5.3.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "node-sass": "^7.0.3",
    "prettier": "^2.8.0",
    "react-is": "^18.2.0"
  }
}
