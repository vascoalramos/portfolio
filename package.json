{
  "name": "portfolio",
  "private": true,
  "description": "Personal Website",
  "version": "3.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vascoalramos/portfolio"
  },
  "bugs": {
    "url": "https://github.com/vascoalramos/portfolio/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build && gh-pages -d public -m 'Deploy new portfolio version'",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,css,md}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "bootstrap": "^5.1.3",
    "gatsby": "^4.13.1",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-image": "^2.13.0",
    "gatsby-plugin-manifest": "^4.13.0",
    "gatsby-plugin-offline": "^5.13.0",
    "gatsby-plugin-react-helmet": "^5.13.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sass": "^5.13.0",
    "gatsby-plugin-sharp": "^4.13.0",
    "gatsby-plugin-sitemap": "^5.13.0",
    "gatsby-remark-code-titles": "^1.1.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^6.13.0",
    "gatsby-source-filesystem": "^4.13.0",
    "gatsby-transformer-remark": "^5.13.0",
    "gatsby-transformer-sharp": "^4.13.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-bootstrap": "^2.3.1",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-reveal": "^1.2.2",
    "react-spring": "^9.4.4",
    "react-transition-group": "^4.4.2",
    "scrollreveal": "^4.0.9",
    "styled-components": "^5.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "node-sass": "^7.0.1",
    "prettier": "^2.6.2",
    "react-is": "^18.1.0"
  }
}
